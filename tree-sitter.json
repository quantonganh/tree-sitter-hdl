{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hdl",
      "camelcase": "Hdl",
      "title": "HDL",
      "scope": "source.hdl",
      "file-types": [
        "hdl"
      ],
      "injection-regex": "^hdl$",
      "class-name": "TreeSitterHdl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Tree-sitter grammar for parsing HDL (nand2tetris)",
    "authors": [
      {
        "name": "Quan Tong",
        "email": "quantonganh@gmail.com",
        "url": "https://quantonganh.com/"
      }
    ],
    "links": {
      "repository": "http://github.com/quantonganh/tree-sitter-hdl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}