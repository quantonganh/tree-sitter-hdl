{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hdl",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "SYMBOL",
      "name": "chip_definition"
    },
    "chip_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "CHIP"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "header",
          "content": {
            "type": "SYMBOL",
            "name": "chip_header"
          }
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "chip_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "chip_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "in_section"
        },
        {
          "type": "SYMBOL",
          "name": "out_section"
        }
      ]
    },
    "in_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "IN"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "input_pin_name",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "FIELD",
                    "name": "input_pin_name",
                    "content": {
                      "type": "SYMBOL",
                      "name": "identifier"
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "out_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "OUT"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "output_pin_name",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              }
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "FIELD",
                    "name": "output_pin_name",
                    "content": {
                      "type": "SYMBOL",
                      "name": "identifier"
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "chip_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "PARTS"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "part"
          }
        }
      ]
    },
    "part": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "chip_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "connection"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "connection"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "connection": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "part_pin",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "chip_pin",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "identifier_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]*"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/**"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "PATTERN",
                  "value": "[^*]|\\*[^/]"
                }
              },
              {
                "type": "STRING",
                "value": "*/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}